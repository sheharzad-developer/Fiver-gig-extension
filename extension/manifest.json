{
    "manifest_version": 3,
    "name": "Fiverr AI Helper",
    "version": "0.1.2",
    "action": { "default_popup": "popup.html" },
    "permissions": ["activeTab", "tabs", "scripting", "storage"],
    "host_permissions": ["https://*.fiverr.com/*", "http://localhost:8000/*"],
    "content_scripts": [{
      "matches": ["https://*.fiverr.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }]
  }